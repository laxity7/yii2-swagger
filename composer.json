{
	"name": "laxity7/yii2-gendoc",
	"description": "Swagger Documentation Generator for Yii2 Framework with swagger v3.0",
	"homepage": "https://github.com/laxity7/yii2-swagger",
	"minimum-stability": "stable",
	"keywords": ["yii2", "extension", "swagger", "yii2 documentation generator"],
	"license": "MIT",
	"authors": [
		{
			"name": "Vlad Varlamov",
			"email": "work@laxity.ru",
			"homepage": "http://laxity.ru/"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-apidoc": "2.*",
		"ext-json": "*"
	},
	"config": {
		"process-timeout": 1800
	},
	"autoload": {
		"psr-4": {
			"laxity7\\yii2\\gendoc\\": ""
		}
	}
}
