{
	"name": "laxity7/yii2-swagger",
	"description": "Swagger for yii2",
	"homepage": "https://github.com/laxity7/yii2-swagger",
	"minimum-stability": "stable",
	"keywords": ["yii2", "extension", "swagger"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Vlad Varlamov",
			"email": "work@laxity.ru",
			"homepage": "http://laxity.ru/"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-apidoc": "2.0.*",
		"phpdocumentor/reflection-docblock": "2.0.4",
		"squizlabs/php_codesniffer": "2.*",
		"phploc/phploc": "3.*",
		"sebastian/phpcpd": "3.*",
		"phpmd/phpmd": "@stable"
	},
	"config": {
		"process-timeout": 1800
	},
	"autoload": {
		"psr-4": {
			"laxity7\\swagger\\": ""
		}
	}
}
